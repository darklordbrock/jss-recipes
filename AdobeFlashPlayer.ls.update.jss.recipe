<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Description</key>
        <string>Downloads Adobe Flash Player installer and extracts the embedded package, uploads to JSS.</string>
        <key>Identifier</key>
        <string>com.github.darklordbrock.jss.update.ls.AdobeFlashPlayer</string>
        <key>Input</key>
        <dict>
            <key>SITE_NAME</key>
            <string>LS</string>
            <key>POLICY_CATEGORY</key>
            <string>Plug-ins</string>
            <key>GROUP_NAME</key>
            <string>Adobe-Flash</string>
            <key>GROUP_TEMPLATE</key>
            <string>%RECIPE_DIR%/AdobeFlashPlayer/LS-Update-Flash-SmartGroupTemplate.xml</string>
            <key>ICON</key>
            <string>%RECIPE_DIR%/icons/AdobeFlashPlayer.png</string>
        </dict>
        <key>MinimumVersion</key>
        <string>0.4.0</string>
        <key>ParentRecipe</key>
        <string>com.github.darklordbrock.jss.put.uits.AdobeFlashPlayer</string>
        <key>Process</key>
        <array>
            <dict>
                <key>Arguments</key>
                <dict>
                    <key>policy_category</key>
                    <string>%POLICY_CATEGORY%</string>
                    <key>self_service_icon</key>
                    <string>%ICON%</string>
                    <key>groups</key>
                    <array>
                        <dict>
                            <key>name</key>
                            <string>%GROUP_NAME%</string>
                            <key>smart</key>
                            <true/>
                            <key>template_path</key>
                            <string>%GROUP_TEMPLATE%</string>
                        </dict>
                    </array>
                </dict>
                <key>Processor</key>
                <string>JSSImporter</string>
            </dict>
        </array>
    </dict>
</plist>
