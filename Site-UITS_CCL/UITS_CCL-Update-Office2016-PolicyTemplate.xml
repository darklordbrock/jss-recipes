<policy>
    <general>
        <site>
          <name>%SITE_NAME%</name>
        </site>
        <name>AutoPkgr-%SITE_NAME%-Update-%VENDER%%NAME%</name>
        <enabled>true</enabled>
        <frequency>Once every week</frequency>
        <category>
            <name>%POLICY_CATEGORY%</name>
        </category>
        <trigger_checkin>true</trigger_checkin>
        <trigger_enrollment_complete>false</trigger_enrollment_complete>
        <trigger_login>false</trigger_login>
        <trigger_logout>true</trigger_logout>
        <trigger_network_state_changed>false</trigger_network_state_changed>
        <trigger_startup>true</trigger_startup>
        <date_time_limitations>
          <no_execute_on>
            <day>Mon</day>
            <day>Tue</day>
            <day>Wed</day>
            <day>Thu</day>
            <day>Fri</day>
            <day>Sat</day>
          </no_execute_on>
          <no_execute_start>7:00 AM</no_execute_start>
          <no_execute_end>11:59 PM</no_execute_end>
        </date_time_limitations>
        <reboot>
          <message>This computer will restart in 5 minutes. Please save anything you are working on and log out by choosing Log Out from the bottom of the Apple menu.</message>
          <startup_disk>Current Startup Disk</startup_disk>
          <specify_startup/>
          <no_user_logged_in>Do not restart</no_user_logged_in>
          <user_logged_in>Do not restart</user_logged_in>
          <minutes_until_reboot>5</minutes_until_reboot>
          <file_vault_2_reboot>false</file_vault_2_reboot>
        </reboot>
    </general>
	<scope>
		<!--Scope added by JSSImporter-->
	</scope>
	<package_configuration>
		<!--Package added by JSSImporter-->
    </package_configuration>
    <scripts>
		<!--Scripts added by JSSImporter-->
    </scripts>
</policy>
