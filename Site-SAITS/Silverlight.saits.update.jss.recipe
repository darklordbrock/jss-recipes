<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Description</key>
        <string>Downloads latest Silverlight installer, copies the package to the build directory with an appended version number, and uploads to the JSS.</string>
        <key>Identifier</key>
        <string>com.github.darklordbrock.jss.updated.saits.Silverlight</string>
        <key>Input</key>
        <dict>
            <key>SITE_NAME</key>
            <string>SA</string>
            <key>POLICY_CATEGORY</key>
            <string>Updates</string>
            <key>POLICY_TEMPLATE</key>
            <string>%RECIPE_DIR%/../Templates-default/UWM-Update-PolicyTemplate.xml</string>
            <key>ICON</key>
            <string>%RECIPE_DIR%/../Silverlight/Silverlight.png</string>
            <key>GROUP_NAME</key>
            <string>%NAME%</string>
            <key>GROUP_TEMPLATE</key>
            <string>%RECIPE_DIR%/../Silverlight/UITS-Update-Silverlight-SmartGroupTemplate.xml</string>
            <key>DESCRIPTION</key>
            <string/>
